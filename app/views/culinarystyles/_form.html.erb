<%= simple_form_for(@culinarystyle) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :piccard, as: :attachinary, label: "Photo vigette" %>
  <%= f.input :banerpic, as: :attachinary, label: "Photo baner" %>
  <%= f.input :fontpic, as: :attachinary, label: "Photo emoticon" %>
  <%= f.submit %>
<% end %>
